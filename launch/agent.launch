<launch>
    <arg name="db_credentials" default="" />

    <node ns="liability_cacher" name="cacher" pkg="liability_cacher" type="cacher_node" output="screen">
        <param name="db_credentials" textfile="$(arg db_credentials)" />
    </node>

    <node ns="liability_cacher" name="web_server" pkg="liability_cacher" type="web_server_node" output="screen">
        <param name="db_credentials" textfile="$(arg db_credentials)" />
    </node>

</launch>
